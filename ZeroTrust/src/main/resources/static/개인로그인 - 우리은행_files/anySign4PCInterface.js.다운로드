{
	// AnySignLite 미지원
	this.setAnySignLite(false, false);

	if (aUserCallback == undefined)
	{
		alert ("callback error");
		return;
	}

	if(AnySign.mAnySignLoad) { this.mAnySignForPC.GetCertInfoEx (aSignedData, "", aOpOption, aInfoOption, 0, aUserCallback ); }
	else{aUserCallback("");}
}

