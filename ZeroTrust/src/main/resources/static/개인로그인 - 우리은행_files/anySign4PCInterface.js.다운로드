var gHostName = document.location.hostname;
var IsOnLine;

if(gHostName.indexOf("w1") > -1 || gHostName.indexOf("wn") > -1) {
	IsOnLine = "FALSE";
}else {
	IsOnLine = "TRUE"; 
}

var xwServiceName = ""; 

var wooriInstallPage = "";
var xwup_aTargetElement = null;

function AnySignInitialize ()
{
	/*------------------------------------------------------------------------
	 * AnySign 기본 설정.
