	if (this.mEncCharset)
		return this.mAnySignForPC.BlockDecEx(aXgateAddress, aCipher, this.mEncCharset, aElement, aUserCallback, aParam);
	else
		return this.mAnySignForPC.BlockDecEx(aXgateAddress, aCipher, "utf-8", aElement, aUserCallback, aParam);
}

