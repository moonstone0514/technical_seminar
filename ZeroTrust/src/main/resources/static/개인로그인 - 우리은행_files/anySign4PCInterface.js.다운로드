		{
			alert ("callback error");
			return;
		}

		return AnySignForPC.DeleteCertificate (1,
											   aSubjectRDN,
											   aUserCallback,
											   aErrCallback);
	}
	
	this.mAnySignForPC.SetStorage( _resultcallback);										
}

